<Window x:Class="SPGen2010.Components.Configures.MsSql.Database.DAL.SP.WResultFormatter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStartupLocation="CenterScreen" WindowStyle="SingleBorderWindow" ResizeMode="NoResize" SizeToContent="WidthAndHeight"
        Title="Database Instance Connector Browser">
    <Grid Height="480" Width="800">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <CheckBox Grid.Column="1" Content="Return Value" Height="16" HorizontalAlignment="Left" Margin="18,26,0,0" Name="checkBox1" VerticalAlignment="Top" />
        <CheckBox Grid.Column="1" Content="Record Affected" Height="16" HorizontalAlignment="Left" Margin="18,48,0,0" Name="checkBox11" VerticalAlignment="Top" />
        <CheckBox Grid.Column="1" Content="Raise Errors" Height="16" HorizontalAlignment="Right" Margin="0,26,120,0" Name="checkBox12" VerticalAlignment="Top" />
        <CheckBox Grid.Column="1" Content="Print Message" Height="16" HorizontalAlignment="Right" Margin="0,48,107,0" Name="checkBox13" VerticalAlignment="Top" />
        <CheckBox Grid.Column="1" Content="Select Tables:" Height="16" HorizontalAlignment="Left" Margin="18,78,0,0" Name="checkBox2" VerticalAlignment="Top" />
        <Label Grid.Column="1" Content="返回值包含下列数据:(打勾)" Height="28" HorizontalAlignment="Left" Margin="12,0,0,0" Name="label1" VerticalAlignment="Top" Width="163" />
        <DataGrid Grid.Column="1" AutoGenerateColumns="False" Margin="12,100,12,43" Name="dataGrid1" />
        <Button Grid.Column="1" Content="保存(_S)" Height="23" HorizontalAlignment="Right" Margin="0,445,12,0" Name="button1" VerticalAlignment="Top" Width="75" />
        <Button Grid.Column="1" Content="添加一个结果集(_I)" Height="23" HorizontalAlignment="Left" Margin="8,445,0,0" Name="button2" VerticalAlignment="Top" Width="123" />
        <Button Grid.Column="1" Content="删除选定结果集(_D)" Height="23" HorizontalAlignment="Right" Margin="0,445,135,0" Name="button3" VerticalAlignment="Top" Width="128" />
        <TextBox Height="442" HorizontalAlignment="Left" Margin="12,26,0,0" Name="textBox1" VerticalAlignment="Top" Width="376" />
        <Label Content="存储过程创建脚本:" Height="28" HorizontalAlignment="Left" Name="label2" VerticalAlignment="Top" Width="163" Margin="9,0,0,0" />
    </Grid>
</Window>
